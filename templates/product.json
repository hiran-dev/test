        "custom_liquid": "{% assign complete_products = product.metafields.custom.frequently_bought.value %}\n\n{% if complete_products != blank %}\n  <div class=\"complete-the-look\" style=\"margin-top:20px;\">\n    <h3 class=\"complete-the-look__title\">Complete the Look</h3>\n\n    <div class=\"complete-the-look__grid\" style=\"display:grid;grid-template-columns:repeat(4,1fr);gap:20px;\">\n      {% for item in complete_products %}\n        {% assign variant = item.selected_or_first_available_variant %}\n\n        {%- assign image = nil -%}\n        {% if item.featured_media %}\n          {% assign image = item.featured_media.preview_image %}\n        {% elsif item.media.size > 0 %}\n          {% assign image = item.media.first.preview_image %}\n        {% elsif item.featured_image %}\n          {% assign image = item.featured_image %}\n        {% endif %}\n\n        {% if item %}\n          <div class=\"complete-the-look__item\" style=\"border:1px solid #eee;padding:10px;border-radius:6px;text-align:center;\">\n            <a href=\"{{ item.url }}\">\n              {% if image %}\n                <img src=\"{{ image | image_url: width: 300 }}\" alt=\"{{ item.title }}\" loading=\"lazy\" style=\"width:100%;height:260px;object-fit:cover;\">\n              {% else %}\n                {{ 'product-1' | placeholder_svg_tag }}\n              {% endif %}\n\n              <p class=\"complete-the-look__product-title\" style=\"font-weight:600;margin:8px 0 4px;\">{{ item.title }}</p>\n\n              <p class=\"complete-the-look__price\" style=\"margin:0;color:#555;\">\n                {% if variant %}\n                  {{ variant.price | money }}\n                {% else %}\n                  {{ item.price | money }}\n                {% endif %}\n              </p>\n            </a>\n          </div>\n        {% endif %}\n      {% endfor %}\n    </div>\n  </div>\n{% endif %}"
